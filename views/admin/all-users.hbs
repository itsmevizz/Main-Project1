  <body>
    <section class="">
      <div class="container">
        <div class="row justify-content-center">
          <div class="col-md-6 text-center mb-5">
          </div>
        </div>
        <div class="row">
          <div class="col-md-12">
            <div class="table-wrap">
              <table class="table table-responsive-xl">
                <thead>
                  <tr>
                    <th>&nbsp;</th>
                    <th>Email</th>
                    <th>Number</th>
                    <th>Username</th>
                    <th>Status</th>
                    <th>Action</th>
                    <th>&nbsp;</th>
                  </tr>
                </thead>
                <tbody>
                  {{#each users}}
                  <tr class="alert" role="alert">
                    <td>{{inc @index}}</td>
                    <td class="d-flex align-items-center">
                      <div class="email">
                        <span>{{this.Email}}</span>
                        <span>{{this._id}}</span>
                      </div>
                    </td>
                    <td>{{this.Number}}</td>
                    <td>{{this.Name}}</td>
                    {{#if this.Status}}
                    <td class="status"><span class="active">Active</span></td>
                    {{else}}
                    <td class="status"><span class="waiting">Blocked</span></td>
                    {{/if}}
                    <td>{{#if this.Status}}
                      <a href="/admin/Block-user?id={{this._id}}" class="btn btn-danger btn-sm"
                        onclick="return confirm('Want to Block {{this.Name}}?')">Block</a>
                      {{else}}
                      <a href="/admin/Unblock-user?id={{this._id}}" class="btn btn-primary btn-sm">Activate</a>
                      {{/if}}
                    </td>
                    <td>
                      <button type="submit" class="close" data-dismiss="alert" aria-label="Close">
                        <a href="/admin/delete-user?id={{this._id}}"
                          onclick="return confirm('Want to delete {{this.Name}}?')"><span aria-hidden="true">Delete <i
                              class="fa fa-close"></i></span></a>
                      </button>
                    </td>
                  </tr>
                  {{/each}}
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </section>
